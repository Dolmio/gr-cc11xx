<?xml version="1.0"?>
<block>
  <name>Data Concatenator</name>
  <key>data_concatenator</key>
  <category>cc11xx</category>
  <import>import cc11xx.data_concatenator</import>
  <make>cc11xx.data_concatenator.data_concatenator($data_to_concat)</make>
    <param>
        <name>Data to concat</name>
        <key>data_to_concat</key>
        <value>[0xff]</value>
        <type>int_vector</type>
    </param>

    <sink>
        <name>in</name>
        <type>message</type>
    </sink>

    <source>
        <name>out</name>
        <type>message</type>
    </source>


</block>